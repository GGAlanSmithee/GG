{
  "name": "gg-rts100k",
  "version": "1.0.0",
  "description": "rts for the browser",
  "main": "dist/gg-100k.js",
  "author": {
    "name": "Alan Smithee",
    "email": "ggnore.alan.smithee@gmail.com"
  },
  "scripts": {
    "babel": "babel build/gg-100k.js > dist/gg-100k.js -s inline",
    "bundle": "rollup -f iife -n GG100k -i build/src/gg-100k.js -e qwest,mainloop.js -g three:THREE -o build/gg-100k.js",
    "clean": "./node_modules/.bin/rimraf ./build/* ./dist/*",
    "typestrip": "flow-typestrip -o build src/*.js src/**/*.js src/**/*.js",
    "uglify": "uglifyjs ./dist/gg-100k.js -o ./dist/gg-100k.min.js -c -m",
    "prebuild": "npm run clean && npm run typestrip && npm run bundle",
    "build": "npm run babel"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:ggnore/100k.git"
  },
  "keywords": [
    "rts",
    "javascript",
    "js"
  ],
  "dependencies": {
    "gg-entities": "^1.0.1",
    "mainloop.js": "^1.0.0",
    "patroljs": "^0.1.0",
    "qwest": "^2.0.7",
    "three": "^0.72.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "eslint": "^1.3.1",
    "flow-bin": "^0.16.0",
    "flow-typestrip": "^0.1.3",
    "gg-entities": "^1.0.2",
    "three": "^0.72.0",
    "uglifyjs": "^2.4.10"
  }
}
