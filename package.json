{
  "name": "gg-rts100k",
  "version": "1.0.0",
  "description": "rts for the browser",
  "main": "dist/gg-100k.js",
  "author": {
    "name": "Alan Smithee",
    "email": "ggnore.alan.smithee@gmail.com"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "babelify": "browserify -t babelify dist/bundle.js --s GG100k > dist/gg-100k.js -d",
    "babel": "babel dist/bundle.js > dist/gg-100k.js -s inline",
    "bundle": "rollup -f iife -n GG100k -g three:THREE,underscore:_,qwest:qwest -e patroljs,mainloop.js -i src/gg-100k.js -o dist/bundle.js",
    "eslint": "eslint src/core",
    "uglify": "uglifyjs dist/gg-100k.js -o dist/gg-100k.min.js -c -m",
    "prebuild": "npm run eslint && npm run clean && npm run bundle",
    "build": "npm run babel",
    "postbuild": "rimraf dist/bundle.js && replace 'undefined.GG100k' 'this.GG100k' dist/gg-100k.js"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:ggnore/100k.git"
  },
  "keywords": [
    "rts",
    "javascript",
    "js"
  ],
  "dependencies": {
    "gg-entities": "^1.0.1",
    "mainloop.js": "^1.0.0",
    "patroljs": "^0.1.0",
    "qwest": "^2.0.7",
    "three": "^0.71.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "eslint": "^1.1.0",
    "esperanto": "^0.7.4",
    "gg-entities": "^1.0.2",
    "replace": "^0.3.0",
    "uglifyjs": "^2.4.10"
  }
}
