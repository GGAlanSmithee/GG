    "babel-plugin-syntax-async-functions": "^6.0.14",
    "babel-plugin-transform-async-to-generator": "^6.0.14",
    "babel-plugin-transform-flow-strip-types": "^6.0.14",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.1.2",
