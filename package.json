{
  "name": "gg-rts100k",
  "version": "1.0.0",
  "description": "rts for the browser",
  "main": "dist/gg-100k.js",
  "author": {
    "name": "Alan Smithee",
    "email": "ggnore.alan.smithee@gmail.com"
  },
  "scripts": {
    "prebuild": "mkdir dist -p",
